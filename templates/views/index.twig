{% extends 'inc/template.twig' %}
{% block conteudo %}
<div class="container">	
		<h1 class="text-center">Login de Administração</h1>
		<form method="post">
			<div class="form-group">
				<label for='login'>Login</label>
				<input type="text" class="form-control" name="login" >
			</div>
			<div class="form-group">
				<label for='senha'>Senha</label>
				<input type="password" class="form-control" name="senha" >
			</div>
			<input type="submit" name="" class="btn blue" value="Entrar">
		</form>
	</div>
{% endblock %}